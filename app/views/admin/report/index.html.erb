s<div class="container cont-t">
	<table class="table table-striped">
		<thead>
			<tr>
				<th>#</th>
				<th>First Name</th>
				<th>Last Name</th>
				<th>Username</th>
			</tr>
		</thead>
		<tbody>
		<% @infos.each do |i| %>
			<tr>
				<th scope="row"><%= i.id%></th>
				<td><%= link_to i.user.name, admin_report_path(i.id)%></td>
				<td><%= link_to i.user.surname, admin_report_path(i.id)%></td>
				<td><%= link_to i.user.last_name, admin_report_path(i.id) %></td>
			</tr>
			<% end %>
		</tbody>
	</table>
</div>